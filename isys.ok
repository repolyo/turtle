#!/bin/bash
PYTHON=
BINDIR=/usr/bin
export OTHERARGS="-d halftone-color" ; export SCREEN_CONTROL_FILE=/usr/share/graphen/screens/color/screen.conf ; export TCASE_MOUNT=$TCASES_MOUNT ; export XITHREADCOUNT=1
# Uncomment this to turn on verbose debugging
#set -v

DIR=$(dirname -- $(readlink -f -- "$0"))

FILENAME=`basename $0`
NAME=${FILENAME%.sh}

#get the emulator from the script name
MASTERS=$DIR/$NAME.checksum

eval $TEST_LOCATION/Rendering/simrunner.py $DEVICE -c $MASTERS $@
